{
    "manifest_version": 2,
  
    "name": "Sale Extention",
    "description": "This extension will help to buy things fast",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },

    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["jquery.min.js", "logic.js"]
        }
      ],

    "permissions": [
     "activeTab"
     ]
  }